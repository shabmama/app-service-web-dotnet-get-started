[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = This ASP.NET sample project is used by the getting started content to demonstrate how easy it is to develop a web app and deploy it to Azure App Service.
[access "refs/*"]
	owner = group user/shabmama
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/shabmama
	label-Verified = -1..+1 group user/shabmama
	push = group user/shabmama
	read = group user/shabmama
	rebase = group user/shabmama
	removeReviewer = group user/shabmama
	submit = group user/shabmama
[access "refs/heads/*"]
	create = group user/shabmama
	push = +force group user/shabmama
	pushMerge = group user/shabmama
	read = group user/shabmama
[access "refs/tags/*"]
	push = group user/shabmama
	push = +force group user/shabmama
